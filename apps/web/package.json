{
  "name": "weltgewebe-web",
  "version": "0.0.0",
  "packageManager": "pnpm@9.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "svelte-kit dev --host",
    "build": "vite build",
    "preview": "vite preview --host",
    "clean": "rimraf node_modules .svelte-kit build",
    "postinstall": "node ./scripts/verify-cookie-version.js",
    "prebuild": "svelte-kit sync",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:ci": "svelte-check --tsconfig ./tsconfig.json --fail-on-warnings",
    "format": "prettier -w .",
    "lint": "prettier -c . && eslint . --max-warnings=0",
    "screenshot": "node scripts/record-screenshot.mjs",
    "pretest": "pnpm build",
    "test": "playwright test",
    "test:report": "playwright show-report",
    "test:ci": "playwright test",
    "report": "playwright show-report playwright-report",
    "test:setup": "node ./node_modules/@playwright/test/cli.js install --with-deps || true",
    "sync": "svelte-kit sync",
    "ci": "node ../../ci/scripts/assert-web-budget.mjs && pnpm lint && pnpm check:ci",
    "dev:cs": "vite dev --host 0.0.0.0 --port 5173",
    "preview:cs": "vite preview --host 0.0.0.0 --port 4173"
  },
  "devDependencies": {
    "@playwright/test": "1.55.1",
    "@sveltejs/adapter-static": "^3.0.3",
    "@sveltejs/kit": "2.5.0",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@types/node": "20.8.5",
    "@typescript-eslint/eslint-plugin": "8.8.0",
    "@typescript-eslint/parser": "8.8.0",
    "eslint": "9.11.1",
    "eslint-plugin-svelte": "2.45.1",
    "globals": "15.9.0",
    "jsdom": "^27.2.0",
    "prettier": "3.3.3",
    "prettier-plugin-svelte": "3.2.6",
    "rimraf": "5",
    "svelte": "^4.2.19",
    "svelte-check": "^4.3.2",
    "typescript": "5.9.2",
    "vite": "^5.4.10"
  },
  "dependencies": {
    "maplibre-gl": "4.7.1"
  },
  "engines": {
    "node": ">=20.19.0"
  }
}